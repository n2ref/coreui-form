{
  "name": "coreui-form",
  "version": "1.0.14",
  "description": "CoreUI form",
  "scripts": {
    "build_all": "gulp build_tpl && gulp build_js && gulp build_js_min && npm run test_js",

    "build_tpl": "gulp build_tpl",
    "build_js": "gulp build_tpl && gulp build_js",
    "build_js_min": "gulp build_tpl && gulp build_js_min",

    "build_watch": "gulp build_watch",
    "test_js": "eslint -c .eslintrc.json --max-warnings 100 --ext .js ./src/js"
  },
  "dependencies": {
    "ejs": "3.1.*",
    "jquery-mask-plugin": "1.14.16"
  },
  "devDependencies": {
    "gulp": "4.*",
    "gulp-concat": "2.*",
    "gulp-sourcemaps": "3.*",
    "gulp-uglify": "3.0.*",
    "gulp-rename": "2.0.0",
    "gulp-babel": "8.0.0",
    "gulp-sass": "5.1.0",
    "gulp-wrap-file": "0.1.2",
    "gulp-html-to-js": "0.0.7",
    "rollup-stream": "1.24.1",
    "vinyl-source-stream": "2.0.0",
    "vinyl-buffer": "1.0.1",
    "sass": "1.68.0",
    "eslint": "8.50.0",
    "@babel/plugin-transform-template-literals": "7.22.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n2ref/coreui-form.git"
  },
  "keywords": [
    "js",
